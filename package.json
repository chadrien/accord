{
  "name": "accord",
  "version": "0.1.0",
  "description": "Discord bot library using streams",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chadrien/accord.git"
  },
  "keywords": [
    "discord",
    "bot",
    "streams",
    "rx",
    "rxjs"
  ],
  "author": "Hadrien de Cuzey <hadrien@decuzey.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chadrien/accord/issues"
  },
  "homepage": "https://github.com/chadrien/accord#readme",
  "main": "dist/index.js",
  "scripts": {
    "pregenerate-docs": "rm -rf docs",
    "generate-docs": "typedoc --out docs --exclude '**/utils/*' --excludeExternals --includeDeclarations --theme markdown",
    "prebuild": "npm run generate-docs && rm -rf dist",
    "build": "tsc",
    "test": "NODE_PATH=. mocha --compilers ts:ts-node/register 'accord/**/*.spec.ts'"
  },
  "dependencies": {
    "rxjs": "5.4.2"
  },
  "devDependencies": {
    "@types/mocha": "2.2.41",
    "@types/node": "8.0.10",
    "discord.js": "~11.1.0",
    "mocha": "3.4.2",
    "ts-node": "3.2.0",
    "tslint": "5.5.0",
    "typedoc": "0.7.1",
    "typedoc-plugin-markdown": "1.0.2",
    "typescript": "2.4.1"
  },
  "peerDependencies": {
    "discord.js": "~11.1.0"
  }
}
